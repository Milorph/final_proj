if (!requireNamespace("BiocManager", quietly = TRUE))
    install.packages("BiocManager")

BiocManager::install("airway")

library(airway)
data("airway")

cts <- assay(airway)
coldata <- as.data.frame(colData(airway))

write.csv(cts, "airway_counts.csv", row.names = TRUE)
write.csv(coldata, "airway_coldata.csv", row.names = TRUE)

exit
exit()
q()
